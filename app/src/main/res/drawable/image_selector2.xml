<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Normal state -->
    <item android:drawable="@drawable/woman_unpressed" />

    <!-- Pressed state -->
    <item android:state_pressed="true" android:drawable="@drawable/woman"/>

    <!-- Disabled state -->
    <item android:state_enabled="false" android:drawable="@drawable/woman_unpressed" />
</selector>